<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Madame Wolker</title>
</head>
<body>
    <header>
        <nav>
            <ul>
                <li>
                    <a href="#">Productos</a>
                </li>
                <li>
                    <a href="#">Usuario</a>
                </li>
                <li>
                    <a href="#">Sobre nosotros</a>
                </li>
                <li>
                    <a href="#">Contactos</a>
                </li>
            </ul>
        </nav>
    </header>
    <main>
        <ul>
            <% productos.forEach(producto => { %>
                <div>
                    <%= producto.nombre %>
                </div>
                <div>
                    <%= producto.tono %>
                </div>
                <div>
                    <%= producto.precio %>
                </div>
                <div>
                    <%= producto.oferta %>
                </div>
                 <div>
                    <%= producto.stock %>
                </div>
            <% }) %>
        </ul>
    </main>
    <a href="/create">Create product</a>
    <footer>
        <p>Derechos reservados &copy; 2023</p>
    </footer>
</body>
</html>